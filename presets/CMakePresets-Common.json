{
	"version": 6,
	"cmakeMinimumRequired": {
		"major": 3,
		"minor": 28,
		"patch": 0
	},
	"configurePresets": [
		{
			"name": "gen_vs2022",
			"hidden": true,
			"generator": "Visual Studio 17 2022",
			"cacheVariables": {
				"CMAKE_EXPORT_COMPILE_COMMANDS": "TRUE"
			}
		},
		{
			"name": "gen_ninja",
			"hidden": true,
			"generator": "Ninja",
			"cacheVariables": {
				"CMAKE_EXPORT_COMPILE_COMMANDS": "TRUE"
			}
		},
		{
			"name": "arch_x86",
			"hidden": true,
			"architecture": {
				"value": "x86",
				"strategy": "external"
			}
		},
		{
			"name": "arch_x64",
			"hidden": true,
			"architecture": {
				"value": "x64",
				"strategy": "external"
			}
		},
		{
			"name": "arch_arm64",
			"hidden": true,
			"architecture": {
				"value": "arm64",
				"strategy": "external"
			}
		},
		{
			"name": "arch_x86_vs",
			"hidden": true,
			"architecture": {
				"value": "Win32",
				"strategy": "set"
			},
			"toolset": {
				"value": "v143,host=x64",
				"strategy": "set"
			}
		},
		{
			"name": "arch_x64_vs",
			"hidden": true,
			"architecture": {
				"value": "x64",
				"strategy": "set"
			},
			"toolset": {
				"value": "v143,host=x64",
				"strategy": "set"
			}
		},
		{
			"name": "arch_arm64_vs",
			"hidden": true,
			"architecture": {
				"value": "ARM64",
				"strategy": "set"
			},
			"toolset": {
				"value": "v143,host=x64",
				"strategy": "set"
			}
		},
		{
			"name": "arch_x64_macos",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_OSX_ARCHITECTURES": "x86_64"
			}
		},
		{
			"name": "arch_arm64_macos",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_OSX_ARCHITECTURES": "arm64"
			}
		},
		{
			"name": "arch_universal_macos",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_OSX_ARCHITECTURES": "arm64;x86_64"
			}
		},
		{
			"name": "config_debug",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Debug"
			}
		},
		{
			"name": "config_release",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Release"
			}
		},
		{
			"name": "compiler_msvc",
			"hidden": true,
			"toolset": {
				"value": "v143,host=x64",
				"strategy": "external"
			},
			"cacheVariables": {
				"CMAKE_C_COMPILER": "cl",
				"CMAKE_CXX_COMPILER": "cl"
			},
			"vendor": {
				"microsoft.com/VisualStudioSettings/CMake/1.0": {
					"hostOS": [
						"Windows"
					]
				}
			}
		},
		{
			"name": "compiler_clang",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_C_COMPILER": "clang",
				"CMAKE_CXX_COMPILER": "clang++"
			}
		},
		{
			"name": "compiler_gcc",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_C_COMPILER": "gcc",
				"CMAKE_CXX_COMPILER": "g++"
			}
		},
		{
			"name": "platform_windows",
			"hidden": true,
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Windows"
			}
		},
		{
			"name": "platform_macos",
			"hidden": true,
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Darwin"
			}
		},
		{
			"name": "platform_linux",
			"hidden": true,
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Linux"
			}
		}
	]
}
