<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>Remove TestInst from the system</title>
    <welcome language="en" mime-type="text/html"><![CDATA[<html><body><br />
<p style="margin: 0px; font: 13px 'Lucida Grande'">This package will remove TestInst and its associated utilities from the system.</p></body></html>]]></welcome>
    <pkg-ref id="${CU_COMPANY_DOMAIN}.${CU_PROJECT_COMPANYNAME}.${CMAKE_INSTALL_DEFAULT_COMPONENT_NAME}.uninstaller.pkg"/>
    <options customize="never" require-scripts="false"/>
    <choices-outline>
        <line choice="default">
            <line choice="${CU_COMPANY_DOMAIN}.${CU_PROJECT_COMPANYNAME}.${CMAKE_INSTALL_DEFAULT_COMPONENT_NAME}.uninstaller.pkg"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="${CU_COMPANY_DOMAIN}.${CU_PROJECT_COMPANYNAME}.${CMAKE_INSTALL_DEFAULT_COMPONENT_NAME}.uninstaller.pkg" visible="false">
        <pkg-ref id="${CU_COMPANY_DOMAIN}.${CU_PROJECT_COMPANYNAME}.${CMAKE_INSTALL_DEFAULT_COMPONENT_NAME}.uninstaller.pkg"/>
    </choice>
    <pkg-ref id="${CU_COMPANY_DOMAIN}.${CU_PROJECT_COMPANYNAME}.${CMAKE_INSTALL_DEFAULT_COMPONENT_NAME}.uninstaller.pkg" version="1.0" onConclusion="none">uninstaller.pkg</pkg-ref>
</installer-gui-script>
